{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["./src"],
        "includePattern": "\\.(jsx|js|ts|tsx)$"
    },
    "plugins": ["../../node_modules/better-docs/typescript"],
    "typescript": {
        "moduleRoot": "."
    },
    "templates": {
        "better-docs": {
            "name": "My React components"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true
    }
}
